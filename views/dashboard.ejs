<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Dashboard</title>
  <!-- Add any additional styles or links to external stylesheets here -->
  <link rel="stylesheet" href="/styles/home.css">
  <link rel="stylesheet" href="/styles/dashboard.css">
</head>
<body>
  <header>
    <h1>Welcome to the Dashboard, <%= username %>!</h1>
    <a href="/home">Home</a>
    <a href="/logout">Logout</a>
  </header>
<!-- Your existing dashboard.ejs content -->

<div class="user-inventory">
  <h3>Your Inventory Items</h3>
  <ul>
    <% userInventoryItems.forEach(item => { %>
      <li><%= item.name %> - <%= item.quantity %> units</li>
    <% }); %>
  </ul>
</div>

<!-- More content if needed -->

  <!-- Add any additional HTML elements or links here -->
</body>
</html>
